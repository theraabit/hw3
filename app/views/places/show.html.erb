<h1> <%= @place.name %> Posts </h1>


<% for post in @place.posts %>
<div class="col-3">
    <h2> <%= post.title %> </h2>
    <p> <%= post.posted_on %> </p>
    <p> <%= post.description %> </p>
<% end %>

<p>
_______________________
</p>
<p>
<a href="/posts/new?place_id=<%= @place.id %>">New Post</a>
</p>
<p>
<a href="/places">Back to All Places </a>
</p>